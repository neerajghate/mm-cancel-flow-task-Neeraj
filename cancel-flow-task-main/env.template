# Supabase Configuration
# Copy this file to .env.local and fill in your actual values

# Your Supabase project URL (found in your Supabase dashboard)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anonymous key (found in your Supabase dashboard)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# Your Supabase service role key (found in your Supabase dashboard)
# This is used for admin operations and should be kept secret
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Optional: Database connection string (for local development)
# DATABASE_URL=postgresql://postgres:password@localhost:54322/postgres
